{
    "issues": {
        "exclude-rules": [
            {
                "path": "main.go",
                "linters": [
                    "gochecknoglobals",
                    "wsl"
                ]
            },
            {
                "path": "_test.go",
                "linters": [
                    "godox",
                    "gomnd",
                    "scopelint"
                ]
            }
        ],
        "exclude-use-default": false,
        "max-issues-per-linter": 0,
        "max-same-issues": 0
    },
    "linters": {
        "enable-all": true,
        "disable": []
    },
    "run": {
        "deadline": "5m",
        "skip-dirs": [
            "reports",
            "scripts"
        ],
        "tests": true
    }
}
